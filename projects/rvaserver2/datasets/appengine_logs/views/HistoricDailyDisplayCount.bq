SELECT count(distinct display_id) as display_count, date
FROM
  (SELECT REGEXP_EXTRACT(protoPayload.resource,r'\/v2\/viewer\/display\/([A-Za-z0-9-]+)') as display_id, date(metadata.timestamp) as date
  FROM table_date_range(appengine_logs.appengine_googleapis_com_request_log_, date_add(current_timestamp(), -90, "DAY"), current_timestamp())
  HAVING display_id is not null)
GROUP BY date
